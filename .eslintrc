{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/prop-types": 0,
        "no-unused-vars": 0,
        "react/no-string-refs": 0,
        "no-mixed-spaces-and-tabs":0,
        "react/jsx-key":0,
        "no-redeclare":0,
        "no-case-declarations":0,
        "react/no-deprecated":0
    }
}